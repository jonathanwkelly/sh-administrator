{
    "name": "jonathanwkelly/sh-administrator",
    "description": "Admin Architect - Administration Framework for Laravel. Create Laravel Admin panels at Reactive speed.",
    "require": {
        "php": ">=5.6.0",
        "laravelcollective/html": "^5.1|^5.2",
        "doctrine/dbal": "~2.5",
        "davejamesmiller/laravel-breadcrumbs": "3.0.*",
        "codesleeve/laravel-stapler": "1.0.*",
        "jonathanwkelly/sh-menus": "dev-master",
        "jonathanwkelly/sh-php-humanizer": "dev-master",
        "creativeorange/gravatar": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jonathanwkelly/sh-menus.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jonathanwkelly/sh-php-humanizer.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Terranet\\Administrator": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "suggest": {
        "adminarchitect/options": "to store and manage settings",
        "adminarchitect/localizer": "to have language detection support by Request Uri, Domain name and Environment",
        "adminarchitect/news": "news module",
        "adminarchitect/pages": "pages module",
        "adminarchitect/contacts": "configurable contact form",
        "terranet/translatable": "to manage multilingual records",
        "terranet/slug": "to allow slugging"
    },
    "minimum-stability": "dev"
}
